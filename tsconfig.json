{
  // https://nuxt.com/docs/guide/concepts/typescript
  "extends": "./.nuxt/tsconfig.json",
  "compilerOptions": {
    "lib": ["DOM", "ESNext.AsyncIterable", "ESNext"],
    "baseUrl": ".",
    "paths": {
      "~/*": [
        "./*"
      ],
      "@/*": [
        "./*"
      ]
    },
    "allowJs": true,
    "strict": true,
    "noEmit": true,
    "sourceMap": true,
    "esModuleInterop": true
  },
  "include": ["./**/*"],
  "exclude": ["node_modules"]
}
